{
    "name": "app",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf node_modules doc data && npm cache clean",
        "clean-install": "npm run clean && npm install",
        "clean-start": "npm run clean-install && npm start",
        "watch": "webpack --watch --progress --profile",
        "build": "rimraf data && webpack --progress --profile --bail",
        "server": "webpack-dev-server --inline --progress --port 8080",
        "webdriver-update": "webdriver-manager update",
        "webdriver-start": "webdriver-manager start",
        "lint": "tslint --force \"src/**/*.ts\"",
        "e2e": "protractor",
        "e2e-live": "protractor --elementExplorer",
        "pretest": "npm run lint",
        "test": "karma start",
        "posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
        "test-watch": "karma start --no-single-run --auto-watch",
        "ci": "npm run e2e && npm run test",
        "docs": "typedoc --options typedoc.json src/app/app.component.ts",
        "start": "npm run server",
        "start:hmr": "npm run server -- --hot",
        "postinstall": "npm run webdriver-update"
    },
    "dependencies": {
        "@angular/common": "^4.4.4",
        "@angular/compiler": "^4.4.4",
        "@angular/core": "^4.4.4",
        "@angular/forms": "^4.4.4",
        "@angular/http": "^4.4.4",
        "@angular/platform-browser": "^4.4.4",
        "@angular/platform-browser-dynamic": "^4.4.4",
        "@angular/router": "^4.4.4",
        "bootstrap-sass": "^3.3.7",
        "core-js": "^2.5.1",
        "eslint": "^4.5.0",
        "jquery": "^3.2.1",
        "npm-cache": "^0.6.5",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.4.3",
        "tslib": "^1.7.1",
        "zone.js": "~0.8.18"
    },
    "devDependencies": {
        "@angularclass/hmr": "~2.1.3",
        "@angularclass/hmr-loader": "^3.0.4",
        "@types/jasmine": "^2.5.53",
        "@types/node": "^7.0.39",
        "@types/selenium-webdriver": "2.53.39",
        "angular2-template-loader": "^0.6.0",
        "autoprefixer": "^6.3.2",
        "awesome-typescript-loader": "^3.2.1",
        "clean-webpack-plugin": "^0.1.16",
        "codelyzer": "3.2.1",
        "copy-webpack-plugin": "^4.0.1",
        "compression-webpack-plugin": "^0.4.0",
        "css-loader": "0.28.3",
        "extract-text-webpack-plugin": "^2.1.2",
        "file-loader": "^0.11.1",
        "html-loader": "^0.4.0",
        "html-minify-loader": "^1.3.0",
        "html-webpack-inline-source-plugin": "0.0.9",
        "html-webpack-inline-svg-plugin": "^0.2.2",
        "html-webpack-plugin": "^2.30.1",
        "istanbul-instrumenter-loader": "^0.2.0",
        "jasmine-core": "^2.6.4",
        "jasmine-spec-reporter": "^3.2.0",
        "json-loader": "^0.5.3",
        "karma": "1.4.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.0.0",
        "karma-jasmine": "^1.0.2",
        "karma-mocha-reporter": "^2.0.3",
        "karma-remap-istanbul": "0.2.1",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "2.0.2",
        "node-sass": "^4.5.2",
        "null-loader": "0.1.1",
        "postcss-loader": "^1.3.3",
        "preload-webpack-plugin": "^2.0.0",
        "protractor": "^5.1.1",
        "raw-loader": "0.5.1",
        "remap-istanbul": "^0.6.4",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "shelljs": "^0.7.8",
        "style-loader": "^0.13.0",
        "ts-helpers": "^1.1.1",
        "tslint": "^5.7.0",
        "tslint-loader": "^3.5.2",
        "typedoc": "^0.7.1",
        "typescript": "^2.5.2",
        "url-loader": "^0.5.8",
        "webpack": "^3.6.0",
        "webpack-concat-plugin": "^1.4.1",
        "webpack-dev-server": "~2.7.1",
        "webpack-raw-bundler": "^1.3.6"
    }
}
